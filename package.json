{
  "name": "gatsby-netlify-ci",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-netlify-ci",
  "author": "Chad Sheets",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^3.13.0",
    "gatsby-plugin-image": "^1.13.0",
    "gatsby-plugin-netlify-cms": "^5.13.0",
    "gatsby-plugin-prettier-eslint": "^1.0.6",
    "gatsby-plugin-sharp": "^3.13.0",
    "gatsby-plugin-styled-components": "^4.13.0",
    "gatsby-source-filesystem": "^3.13.0",
    "gatsby-transformer-remark": "^4.10.0",
    "gatsby-transformer-sharp": "^3.13.0",
    "netlify-cms-app": "^2.15.41",
    "prettier": "^2.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ],
    "rules": {
      "react/prop-types": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false
  }
}
